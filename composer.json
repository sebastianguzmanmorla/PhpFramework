{
    "name": "sebastianguzmanmorla/php-framework",
    "description": "A custom PHP Framework",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "ext-pgsql": "*",
        "ext-mysqli": "*",
        "ext-bcmath": "*",
        "ext-zip": "*",
        "ext-gd": "*",
        "hashids/hashids": "^5.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "firebase/php-jwt": "^6.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "homepage": "https://github.com/sebastianguzmanmorla/PhpFramework",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpFramework\\": ["src/"]
        }
    },
    "authors": [
        {
            "name": "Sebastián Enrique Guzmán Morla",
            "email": "sebastian.guzman@outlook.com"
        }
    ],
    "scripts": {
        "fix": [
            "php-cs-fixer fix"
        ]
    }
}
